    "fixWebpackChunksIssue": true,
    "removeBlobs": true,
    "inlineCss": true,
    "include": [
      "/about",
      "/product",
      "/product/list",
      "/estimate",
      "/video"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
